<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="MSL BDD Test Suite" verbose="1" parallel="classes"
	thread-count="1" configfailurepolicy="continue">
	<listeners>
		<listener class-name="runner.NGTestListener"/>
		</listeners>
	<test name="User Group Test" annotations="JDK"
		preserve-order="true">
		<classes>
			<class name="runner.TestRunner" />
		</classes>
	</test>
</suite>